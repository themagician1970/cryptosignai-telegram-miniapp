{
  "name": "cryptosignai-mini-app",
  "version": "1.0.0",
  "description": "CryptoSignAI Telegram Mini App for trading analysis",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "echo 'Building Mini App...' && npm run copy-assets",
    "copy-assets": "cp -r telegram-mini-app/* public/",
    "deploy": "npm run build && echo 'Deploy to your hosting service'"
