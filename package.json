  "keywords": [
    "telegram",
    "mini-app",
    "crypto",
    "trading",
    "ai",
    "analysis"
  ],
  "author": "CryptoSignAI Team",
  "license": "MIT",
  "dependencies": {
    "@mathieuc/tradingview": "^3.5.1",
    "@twa-dev/sdk": "^7.0.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "node-telegram-bot-api": "^0.64.0",
    "sharp": "^0.33.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
